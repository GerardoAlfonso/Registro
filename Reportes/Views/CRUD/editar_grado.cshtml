
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/css/bootstrap.min.css">

</head>
<body>

    <section>
        <header>
            <div class="container">
                <div class="title text-center mt-5">
                    <p class="h4">Editar grado</p>
                    <p>CRUD correspondiente para la tabla alumnos.</p>
                </div>
        </header>
    </section>

    <section>

        <div class="container mt-5">

            <form>
                <div class="form-group">
                    <label for="recipient-name" class="col-form-label">Nombre:</label>
                    <input type="text" class="form-control" id="recipient-name" maxlength="100" minlength="10" required>

                </div>

                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal" onclick="window.history.back();">Cancelar</button>
                    <input type="submit" class="btn btn-success" value="Crear registro">
                </div>

            </form>

        </div>
    </section>
















    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="js/app.js" charset="utf-8"></script>
</body>
</html>


